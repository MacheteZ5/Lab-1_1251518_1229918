
@{
    ViewBag.Title = "Download";
}
<h1>MIS COMPRESIONES</h1>
<h2>El archivo se proceso correctamente</h2>

<body>
    <p>Por favor, presione el nombre del archivo para iniciar la descarga.</p>
    <div>
        @foreach (var item in ViewData.Model)
        {
            @Html.ActionLink((string)item, "DownloadFile", new { filename = item });
            <br>
        }
    </div>

    <form action="~/Home">
        <input id="volver" type="submit" value="Volver A Página Principal" />
    </form>
</body>